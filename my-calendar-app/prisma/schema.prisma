generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Task {
  id           Int      @id @default(autoincrement())
  taskName     String
  description  String?
  priority     String
  project      String
  deadline     DateTime
  timeRequired String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}